{
  "name": "draaw",
  "version": "0.0.0",
  "description": "A 2D Canvas engine",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/draaw#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/draaw/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/etienne-dldc/draaw.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/draaw.esm.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "example": "example",
    "test": "tests"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --format esm,cjs --dts",
    "codecov": "codecov",
    "example:start": "vite",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn run build && np",
    "test": "yarn run lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "resolutions": {
    "typescript": "4.3.4"
  },
  "dependencies": {
    "@juggle/resize-observer": "^3.3.1",
    "suub": "^3.1.3",
    "vite": "^2.8.5"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "codecov": "^3.8.3",
    "core-js": "3.21.1",
    "democrat": "^4.0.0-7",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.5.1",
    "mobx": "^6.4.2",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "tsup": "^5.11.13",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "democrat": "4.0.0-5",
    "mobx": "^6.1.8"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "np": {
    "yarn": true,
    "cleanup": false
  }
}
