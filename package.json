{
  "name": "draaw",
  "version": "0.0.0",
  "description": "A 2D Canvas engine",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/draaw#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/draaw/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/etienne-dldc/draaw.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/draaw.esm.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "example": "example",
    "test": "tests"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --format esm,cjs --dts",
    "codecov": "codecov",
    "example:start": "vite",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn run build && np",
    "test": "yarn run lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "resolutions": {
    "typescript": "4.3.4"
  },
  "dependencies": {
    "@preact/signals-core": "^1.2.3",
    "miid": "^6.0.3",
    "suub": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "codecov": "^3.8.3",
    "core-js": "3.27.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.0",
    "np": "^7.6.3",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "tslib": "^2.4.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "np": {
    "yarn": true,
    "cleanup": false
  }
}
